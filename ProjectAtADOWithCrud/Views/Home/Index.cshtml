@model IEnumerable<ProjectAtADOWithCrud.Models.StudentCourse>
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">StudentCourse</h1>
   <table class="table table-bordered">
       <tr>
           <th>Id</th>
           <th>StudName</th>
           <th>Email</th>
           <th>Contact</th>
           <th>Course</th>
       </tr>
       @foreach(var stud in Model)
            {
           <tr>
               <td>@stud.Id</td>
               <td>@stud.StudName</td>
               <td>@stud.Email</td>
               <td>@stud.Contact</td>
               <td>@stud.Course</td>
           </tr>
                    }
   </table>
</div>
